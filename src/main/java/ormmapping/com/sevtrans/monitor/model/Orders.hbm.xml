<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sevtrans.monitor.model.Orders" table="Orders" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="comment" column="Comment" type="string" length="255" not-null="true" lazy="false"/>
		<set name="ORM_OrderDetail" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="OrderId" not-null="true"/>
			<one-to-many class="com.sevtrans.monitor.model.OrderDetail"/>
		</set>
	</class>
</hibernate-mapping>
