<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sevtrans.monitor.model.OrderDetail" table="Order_Detail" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="orders" cascade="lock" column="OrderId" class="com.sevtrans.monitor.model.Orders" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="lineNo" column="Line_No" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="article" column="Article" type="string" length="255" not-null="true" lazy="false"/>
		<property name="qty" column="Qty" type="double" not-null="true" lazy="false"/>
		<property name="um" column="Um" type="string" length="255" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
